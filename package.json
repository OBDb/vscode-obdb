{
  "name": "signalid-hover-info",
  "displayName": "OBDb",
  "description": "Shows model year support information when hovering over signal IDs",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "contributes": {
    "capabilities" : {
        "hoverProvider" : "true"
    },
    "languages": [
      {
        "id": "json",
        "extensions": [
          ".json"
        ]
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.6.3"
  }
}
